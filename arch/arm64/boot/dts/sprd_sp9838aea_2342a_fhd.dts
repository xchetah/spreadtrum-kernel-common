			auto_gate_bit = <0x00000100>;//bit8
			force_gate_bit = <0x00000200>;//bit9
			emc_en_bit = <0x00000008>;//bit3
			reg = <0 0x20A00000 0 0x1000>, //gspn module ctl reg
