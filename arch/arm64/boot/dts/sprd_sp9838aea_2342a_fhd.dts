			<0 0x20b00000 0 0x8000>,  //pgt
			<0 0x20b08000 0 0x8000>;  //ctrl_reg
		reg_name = "iova","pgt","ctrl_reg";
                 clock-names = "clk_gsp_emc","clk_153m6","clk_gsp0";
                 clocks = <&clk_gsp_emc>, <&clk_153m6>,<&clk_gsp0>;
